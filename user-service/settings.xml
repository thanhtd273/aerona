<settings xmlns="http://maven.apache.org/SETTINGS/1.1.0">
    <servers>
        <server>
            <id>gitlab-maven</id>
            <configuration>
                <httpHeaders>
                    <property>
                        <name>Private-Token</name>
                        <value>${GITLAB_PAT}</value>
                    </property>
                </httpHeaders>
            </configuration>
        </server>
    </servers>
</settings>